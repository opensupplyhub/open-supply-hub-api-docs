{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "description": "This schema represents a geocoded data",
    "properties": {
        "geocoded_address": {
            "type": "string",
            "example": "5900 S Santa Fe Dr, Littleton, CO 80120, USA",
            "description": "The address that was geocoded during processing data"
        },
        "geocoded_geometry": {
            "type": "object",
            "allOf": [
                {
                    "properties": {
                        "type": {
                            "type": "string",
                            "example": "Point",
                            "description": "The type of geocoded data that was processed"
                        },
                        "coordinates": {
                            "type": "object",
                            "allOf": [
                                {
                                    "properties": {
                                        "type": "array",
                                        "items": {
                                            "type": "number",
                                            "example": [
                                                -105.0187252,
                                                39.6080824
                                            ]
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            ]
        }
    }
}