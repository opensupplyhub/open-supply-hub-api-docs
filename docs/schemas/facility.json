{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "description": "This schema represents a facility",
  "properties": {
    "id": {
      "type": "string",
      "description": "The unique identifier for a facility"
    },
    "name": {
      "type": "string",
      "description": "The name of the facility"
    }
  },
  "required": ["id", "name"]
}
